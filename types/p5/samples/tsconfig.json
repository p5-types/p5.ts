{
    "compilerOptions": {
        "module": "commonjs",
        "lib": ["dom", "es5"],
        "noImplicitAny": false,
        "noImplicitThis": false,
        "strictNullChecks": false,
        "strictFunctionTypes": false,
        "baseUrl": "../../",
        "typeRoots": [
            "../../"
        ],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "**/*.ts"
    ]
}
